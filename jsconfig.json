{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@config": ["config/index.js"],
      "@assets/*": ["assets/*"],
      "@views/*": ["views/*"],
      "@store/*": ["store/*"],
      "@sdk/*": ["sdk/*"],
      "@app/*": ["App/*"],
      "@/*": ["*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*"]
}
